# Configuration Supabase pour la production
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Configuration de l'application
VITE_APP_NAME=Kinshasa-Alerte
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# URLs de redirection pour l'authentification
VITE_AUTH_REDIRECT_URL=https://your-domain.com
VITE_RESET_PASSWORD_URL=https://your-domain.com/auth?reset=true

# Configuration des emails
VITE_EMAIL_FROM=noreply@your-domain.com
VITE_EMAIL_SUPPORT=support@your-domain.com

# Configuration de monitoring (optionnel)
VITE_SENTRY_DSN=your-sentry-dsn
VITE_ANALYTICS_ID=your-analytics-id

# Configuration de sécurité
VITE_CSP_NONCE=your-csp-nonce
VITE_HSTS_MAX_AGE=31536000 